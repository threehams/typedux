{
  "name": "reverie-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p . && cross-env NODE_ENV=production webpack --config tsDist/webpack.config.js",
    "lint": "tslint src/**/*.ts{,x}",
    "lint:fix": "tslint src/**/*.ts{,x} --fix",
    "postinstall": "node scripts/postinstall.js",
    "test": "jest --watch",
    "test:ci": "jest",
    "test:coverage": "jest --coverage",
    "test:coverage:open": "opener ./coverage/lcov-report/index.html",
    "start": "cross-env NODE_ENV=development PORT=8080 nodemon ./server.js",
    "server": "cross-env NODE_ENV=production server"
  },
  "jest": {
    "collectCoverageFrom": ["**/!(*.d.ts)"],
    "globals": {
      "__TS_CONFIG__": "tsconfigTest.json"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testResultsProcessor": "../node_modules/ts-jest/coverageprocessor.js",
    "transform": {
      ".(ts|tsx)": "../node_modules/ts-jest/preprocessor.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/threehams/reverie-client.git"
  },
  "dependencies": {
    "@types/compression": "0.0.33",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.13",
    "@types/react": "^15.0.22",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.39",
    "@types/react-router": "^4.0.4",
    "@types/recompose": "^0.22.0",
    "@types/redux-thunk": "^2.1.31",
    "@types/reselect": "^2.2.0",
    "@types/seamless-immutable": "^6.1.2",
    "@types/webpack": "^2.2.15",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "cross-env": "^4.0.0",
    "express": "^4.15.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-test-renderer": "^15.5.4",
    "recompose": "^0.23.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0",
    "seamless-immutable": "^7.1.2",
    "ts-jest": "^19.0.10",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.2",
    "typescript": "2.3.0",
    "webpack": "^2.4.1"
  },
  "devDependencies": {
    "@types/enzyme": "^2.7.9",
    "@types/jest": "^19.2.2",
    "enzyme": "^2.8.2",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "opener": "^1.4.3",
    "tslint": "^5.1.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/__test__/**/*",
      "src/fixtures/**/*",
      "src/*.js"
    ]
  }
}
